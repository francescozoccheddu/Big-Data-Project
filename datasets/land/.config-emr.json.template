  "featurization": {
    "make": {
      "descriptor": {
        "algorithm": "sift",
        "maxFeatureCount": 200,
        "minFeatureCount": 50
      },
      "codebook": {
        "size": 700,
		"maxIterations": 20,
		"initSteps": 3,
        "assignNearest": true
      }
    }
  },
  "training": {
	"save": "./model",
    "make": {
      "algorithm": "nn"
    }
  },
